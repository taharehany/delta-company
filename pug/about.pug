extend index

block website_title
    title #{website_title} | من نحن

block home_block
    //breadcrumb
    nav(aria-label="breadcrumb" class="main-breadcrumb" style="background-image:url('../images/bgs/offer-bg.webp')")
        div(class="container")
            div(class="breadcrumb-title")
                h2 من نحن
            nav(aria-label="breadcrumb")
                ol(class="breadcrumb")
                    li(class="breadcrumb-item")
                        a(href="index.html") الرئيسية
                    li(class="breadcrumb-item active", aria-current="page") من نحن
    //breadcrumb
    
    //welcome
    section(class="welcome")
        div(class="container")
            div(class="main-title" data-wow-duration="1.3s" data-wow-delay=".3s")
                span(class="top-title") أهلا بكم في موقعنا
                h2 الحل السريع للسيارات
                div(class="separator")
                p تاجر السيارات هو سمة ووردبريس المتميزة الأكثر جاذبية وإبداعًا وحداثة ومتعددة الأغراض. مناسبة لأي مواقع تجار سيارات أو مواقع أعمال أو شركات. تم إنشاء السمة خصيصًا لتجار السيارات وبائعي السيارات ومحطات خدمة السيارات وورش الميكانيكا وتجار السيارات بالتجزئة.
            div(class="content")
                div(class="row")
                    div(class="col-lg-3 col-md-6 col")
                        div(class="welcome-box")
                            div(class="icon")
                                img(src="images/welcome/01.png", class="img-fluid" alt="")
                            div(class="text")
                                h3 عنوان النص 1
                                p عند موافقه العميل المبدئيه على التصميم يتم ازالة هذا النص من التصميم ويتم وضع النصوص النهائية المطلوبة للتصميم
                    div(class="col-lg-3 col-md-6 col")
                        div(class="welcome-box")
                            div(class="icon")
                                img(src="images/welcome/02.png", class="img-fluid" alt="")
