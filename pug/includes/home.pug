//bmi calculate
section(class="bmi")
    div(class="container")
        div(class="content")
            div(class="row")
                div(class="col-lg-6")
                    div(class="bmi-result")
                        table(class="table table-dark table-hover")
                            thead
                                tr(class="bmi-heading")
                                    th(class="pt-class") BMI
                                    th Weight Status
                            tbody
                                tr
                                    td Below 18.5
                                    td Underweight
                                tr
                                    td 18.5 - 24.9
                                    td Healthy
                                tr
                                    td 25.0 - 29.9
                                    td Overweight
                                tr
                                    td 30.0 - and Above
                                    td Obese
                        div(class="bmi-info")
                            p
                                //- strong BMR 
                                //- | Metabolic Rate / 
                                strong your BMI result:
                                span(class="bmi-text")
                            p 
                                strong your BMI number:
                                span(class="bmi-number")
                div(class="col-lg-6")
                    div(class="main-title")
                        span(class="top-title") Body mass index
                        h2 What is your bmi
                    div(class="bmi-form")
                        form(class="needs-validation" action="" novalidate)
                            div(class="row")
                                div(class="col-lg-6 col-md-6 col-sm-6")
                                    div(class="form-group position-relative")
                                        input(type="number" max="250" min="50" class="form-control height" placeholder="Height (cm)" required)
                                div(class="col-lg-6 col-md-6 col-sm-6")
                                    div(class="form-group position-relative")
                                        input(type="number" max="1000" min="1" class="form-control weight" placeholder="Weight (kg)" required)
                                div(class="col-lg-6 col-md-6 col-sm-6")
                                    div(class="form-group position-relative")
                                        input(type="number" max="200" min="1" class="form-control age" placeholder="Age (year)" required)
                                div(class="col-lg-6 col-md-6 col-sm-6")
                                    div(class="select-area")
                                        select(class="select gender")
                                            option(disabled, selected, class="hidden-option") Gender
                                            option(value="male") male
                                            option(value="female") female
                                //- div(class="col-lg-12")
                                    div(class="select-area")
                                        select(class="select")
                                            option(disabled, selected, class="hidden-option") Select an activity factor
                                            option(value="1") factor 01
                                            option(value="2") factor 02
                            div(class="bmi-btn")
                                div(class="outer-btn")
                                    button(class="btn calculate" type="submit")
                                        span calculate
//bmi calculate

