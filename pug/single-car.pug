                    a(href="#!")
                        i(class="bi bi-facebook")
                    a(href="#!")
                        i(class="bi bi-twitter")
                    a(href="#!")
                        i(class="bi bi-instagram")
                    a(href="#!")
                        i(class="bi bi-linkedin")
                div(class="row")
                    div(class="col-lg-8")
                        div(class="single-car-box")
                            div(class="single-car-images")
                                div(class="slider-for")
                                    div(class="item")
                                        a(data-fancybox="demo", data-src="images/car/01.jpg", data-caption="")
