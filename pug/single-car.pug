                                    div(class="item")
                                        img(src="images/car/01.jpg", class="img-fluid" alt="")
                                    div(class="item")
                                        img(src="images/car/02.jpg", class="img-fluid" alt="")
                                    div(class="item")
                                        img(src="images/car/03.jpg", class="img-fluid" alt="")
                                    div(class="item")
                                        img(src="images/car/04.jpg", class="img-fluid" alt="")                            
                            //features
                            div(class="single-car-features")
                                div(class="subtitle") 
                                    h3 Features
                                    span(class="line")
                                ul(class="m-0")
