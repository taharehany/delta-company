                                        a(data-fancybox="demo", data-src="images/car/03.jpg", data-caption="")
                                            img(src="images/car/03.jpg", class="img-fluid" alt="")
                                    div(class="item")
                                        a(data-fancybox="demo", data-src="images/car/04.jpg", data-caption="")
                                            img(src="images/car/04.jpg", class="img-fluid" alt="")
